<Page x:Class="SpriteGenerator.Pages.Templates"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SpriteGenerator.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Templates">

    <Grid>
        <StackPanel Orientation="Vertical" Margin="10">
            <Button Name="UploadButton" Content="Upload File" Click="UploadButton_Click" />
            <ComboBox Name="UploadedFilesComboBox" Margin="0,10,0,0" />
        </StackPanel>
    </Grid>
</Page>
